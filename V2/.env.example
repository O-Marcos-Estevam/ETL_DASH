# ==============================================
# ETL Dashboard V2 - Environment Variables
# ==============================================
# Copy this file to .env and adjust values as needed

# Backend Configuration
ETL_HOST=0.0.0.0
ETL_PORT=4001
ETL_DEBUG=false
ETL_LOG_LEVEL=INFO

# CORS - Origens permitidas (separadas por virgula)
ETL_CORS_ORIGINS=http://localhost:4000,http://127.0.0.1:4000

# Execution Settings
ETL_TIMEOUT=3600
ETL_POLL_INTERVAL=2.0

# Frontend Configuration (Vite)
VITE_API_URL=http://localhost:4001/api
VITE_WS_URL=ws://localhost:4001
